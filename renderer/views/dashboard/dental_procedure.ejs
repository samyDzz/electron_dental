<body>
	
	<div class="container-fluid">
		<div class="row">
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
				<form id="procedures-form">
					<div class="_control">
						<label>NOM DES PROCÉDURES</label>
						<input type="text" name="search" placeholder="" />
						<p class="err err-name">Veuillez insérer un nom de procédure valide</p>
					</div>
					<div class="_control">
						<label>PRIX</label>
						<input type="number" name="search" placeholder="0" />
						<p class="err err-card">Veuillez insérer un prix valide</p>
					</div>
					<div class="_control control_options">
						<button id="save_patient">
							<img src="../../assets/images/icons/diskette.svg" alt="SAVE" />
							<span>ENREGISTRER<span>
						</button>
						<button id="reset_patient">
							<img src="../../assets/images/icons/reset.svg" alt="RESET" />
							<span>RÉINITIALISER<span>
						</button>
						<button id="close_patient">
							<img src="../../assets/images/icons/close.svg" alt="CLOSE" />
							<span>FERMER<span>
						</button>
					</div>
				</form>	
			</div>
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 " style="margin-top: 25px;">
				<div class="table-box">	
					<div class="no-procedures" style="display: none;">
						<p>
							Rien à afficher
						</p>
					</div>
					<table class="table table-striped">
						<thead>
							<tr>
								<th scope="col" style="min-width: 70px;"></th>
								<th scope="col" style="min-width: 130px;">PROCÉDURES DENTAIRES</th>
								<th scope="col">PRIX</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td scope="col">
									<img src="../../assets/images/icons/pencil.svg" class="edit-patient" alt="edit patient" />
									<img src="../../assets/images/icons/delete-table.svg" class="delete-patient" alt="edit patient" />
								</td>
								<td scope="col">Des dents d'anger</td>
								<td scope="col">1700</td>
							</tr>
							<tr>
								<td scope="col">
									<img src="../../assets/images/icons/pencil.svg" class="edit-patient" alt="edit patient" />
									<img src="../../assets/images/icons/delete-table.svg" class="delete-patient" alt="edit patient" />
								</td>
								<td scope="col">Dolimus Manus</td>
								<td scope="col">1450</td>
							</tr>
							<tr>
								<td scope="col">
									<img src="../../assets/images/icons/pencil.svg" class="edit-patient" alt="edit patient" />
									<img src="../../assets/images/icons/delete-table.svg" class="delete-patient" alt="edit patient" />
								</td>
								<td scope="col">Sociation Moliere</td>
								<td scope="col">1230</td>
							</tr>
							<tr>
								<td scope="col">
									<img src="../../assets/images/icons/pencil.svg" class="edit-patient" alt="edit patient" />
									<img src="../../assets/images/icons/delete-table.svg" class="delete-patient" alt="edit patient" />
								</td>
								<td scope="col">Loupes xxxx</td>
								<td scope="col">950</td>
							</tr>
							<tr>
								<td scope="col">
									<img src="../../assets/images/icons/pencil.svg" class="edit-patient" alt="edit patient" />
									<img src="../../assets/images/icons/delete-table.svg" class="delete-patient" alt="edit patient" />
								</td>
								<td scope="col">Moon Red</td>
								<td scope="col">2650</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>

